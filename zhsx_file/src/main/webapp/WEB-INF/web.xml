<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>FileUploadServlet</servlet-name>
		<display-name>FileUploadServlet</display-name>
		<description></description>
		<servlet-class>com.zzrenfeng.fileupload.FileUploadServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FileUploadActionServlet</servlet-name>
		<display-name>FileUploadActionServlet</display-name>
		<description></description>
		<servlet-class>com.zzrenfeng.fileupload.FileUploadActionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FormatConversionServlet</servlet-name>
		<display-name>FormatConversionServlet</display-name>
		<description></description>
		<servlet-class>com.zzrenfeng.videoTranscode.FormatConversionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FileUploadNotBlockServlet</servlet-name>
		<display-name>FileUploadNotBlockServlet</display-name>
		<description></description>
		<servlet-class>com.zzrenfeng.fileupload.FileUploadNotBlockServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>OfficeConvertPdfServlet</servlet-name>
		<display-name>OfficeConvertPdfServlet</display-name>
		<description></description>
		<servlet-class>com.zzrenfeng.officeConertPdf.OfficeConvertPdfServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VideoScreenshotServlet</servlet-name>
		<display-name>VideoScreenshotServlet</display-name>
		<description></description>
		<servlet-class>com.zzrenfeng.videoTranscode.VideoScreenshotServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VideoFileDownloadServlet</servlet-name>
		<display-name>VideoFileDownloadServlet</display-name>
		<description></description>
		<servlet-class>com.zzrenfeng.fileDownload.VideoFileDownloadServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FileUploadServlet</servlet-name>
		<url-pattern>/FileUploadServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FileUploadActionServlet</servlet-name>
		<url-pattern>/FileUploadActionServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormatConversionServlet</servlet-name>
		<url-pattern>/FormatConversionServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FileUploadNotBlockServlet</servlet-name>
		<url-pattern>/FileUploadNotBlockServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OfficeConvertPdfServlet</servlet-name>
		<url-pattern>/OfficeConvertPdfServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VideoScreenshotServlet</servlet-name>
		<url-pattern>/VideoScreenshotServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VideoFileDownloadServlet</servlet-name>
		<url-pattern>/VideoFileDownloadServlet</url-pattern>
	</servlet-mapping>
</web-app>
